<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vladiz.registro">

<!-- SE USA ESTA LINEA DE CODIGO PARA DAR PERMISOS A LA APLICACION SOBRE EL USO DE DIFERENTES CARARTERISTICAS EN EL CELULAR
EN ESTE CASO NECESITAREMOS DARLE PERMISOS DE INTERNET-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!---↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑-->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".usuario" />
        <activity android:name=".lista"></activity>
    </application>

</manifest>